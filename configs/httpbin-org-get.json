{
  "url": "https://httpbin.org/get",
  "selectors": {
    "title": "title, h1",
    "content": "main, .content, .article, #content",
    "links": "a:multiple"
  },
  "headers": {
    "accept": "application/json"
  },
  "options": {
    "timeout": 30000,
    "retries": 3,
    "headless": true,
    "screenshot": false
  }
}
