{
  "url": "https://example.com/api/data",
  "selectors": {
    "title": "title, h1",
    "content": "main, .content, .article, #content",
    "links": "a:multiple"
  },
  "headers": {
    "accept": "application/json",
    "content-type": "application/json",
    "authorization": "Bearer token123"
  },
  "options": {
    "timeout": 30000,
    "retries": 3,
    "headless": true,
    "screenshot": false
  }
}
