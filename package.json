{
  "name": "universal-web-crawler",
  "version": "1.0.0",
  "description": "A universal web crawler with configurable selectors, headers, and cookie management",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "dev": "npx tsx src/index.ts",
    "start": "node dist/index.js",
    "crawl": "npx tsx src/cli.ts crawl",
    "doctor": "npx tsx src/cli.ts doctor",
    "list": "npx tsx src/cli.ts list",
    "validate": "npx tsx src/cli.ts validate",
    "curl2config": "npx tsx src/cli.ts curl2config",
    "pipeline": "npx tsx src/cli-pipeline.ts",
    "pipeline:run": "npx tsx src/cli-pipeline.ts run",
    "pipeline:stats": "npx tsx src/cli-pipeline.ts stats",
    "pipeline:clean": "npx tsx src/cli-pipeline.ts clean",
    "clean": "rimraf output dist",
    "clean:output": "rimraf output",
    "clean:dist": "rimraf dist",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "typecheck": "npx tsc --noEmit",
    "generate:mappings": "tsx scripts/generate-category-symbol-mapping.ts",
    "update:stockcodes": "tsx scripts/update-stockcodes.ts",
    "update:stockcodes:tw": "tsx scripts/update-stockcodes.ts --market=tw",
    "update:stockcodes:us": "tsx scripts/update-stockcodes.ts --market=us",
    "update:stockcodes:jp": "tsx scripts/update-stockcodes.ts --market=jp",
    "update:stockcodes:dry": "tsx scripts/update-stockcodes.ts --dry-run",
    "generate:configs": "npm run generate:yahoo-tw-configs && npm run generate:yahoo-jp-configs && npm run generate:yahoo-us-configs",
    "generate:configs:all": "npm run update:stockcodes && npm run generate:configs",
    "generate:yahoo-tw-configs": "tsx scripts/generate-yahoo-tw-configs.ts",
    "generate:yahoo-jp-configs": "tsx scripts/generate-yahoo-jp-configs.ts",
    "generate:yahoo-us-configs": "tsx scripts/generate-yahoo-us-configs.ts",
    "import:fundamental": "tsx scripts/import-fundamental-api.ts",
    "import:fundamental:file": "tsx scripts/import-fundamental-api.ts --file",
    "import:fundamental:batch": "tsx scripts/import-fundamental-api.ts --dir output/ --pattern '*'",
    "import:fundamental:us": "tsx scripts/import-fundamental-api.ts --dir output/ --pattern '*us*'",
    "import:fundamental:tw": "tsx scripts/import-fundamental-api.ts --dir output/ --pattern '*tw*'",
    "import:fundamental:jp": "tsx scripts/import-fundamental-api.ts --dir output/ --pattern '*jp*'",
    "sync:labels": "tsx scripts/sync-category-labels-simple.ts",
    "sync:labels:simple": "tsx scripts/sync-category-labels-simple.ts",
    "sync:labels:dry": "tsx scripts/sync-category-labels-simple.ts --dry-run",
    "sync:labels:progress": "tsx scripts/sync-category-labels-simple.ts --progress",
    "sync:labels:chunk": "tsx scripts/sync-category-labels-simple.ts --chunk-size=100 --progress",
    "clear:labels": "tsx scripts/clear-industry-labels.ts --confirm",
    "clear:labels:dry": "tsx scripts/clear-industry-labels.ts --dry-run",
    "clear:labels:hard": "tsx scripts/clear-industry-labels.ts --confirm --force-hard-delete",
    "clear:labels:hard:dry": "tsx scripts/clear-industry-labels.ts --dry-run --force-hard-delete",
    "clear:labels:tpe": "tsx scripts/clear-industry-labels.ts --market=TPE --confirm",
    "clear:labels:tpe:hard": "tsx scripts/clear-industry-labels.ts --market=TPE --confirm --force-hard-delete",
    "clear:labels:us": "tsx scripts/clear-industry-labels.ts --market=US --confirm",
    "clear:labels:us:hard": "tsx scripts/clear-industry-labels.ts --market=US --confirm --force-hard-delete",
    "clear:labels:jp": "tsx scripts/clear-industry-labels.ts --market=JP --confirm",
    "clear:labels:jp:hard": "tsx scripts/clear-industry-labels.ts --market=JP --confirm --force-hard-delete",
    "import:symbols": "tsx scripts/import-symbols.ts --batch-size=30",
    "import:symbols:dry": "tsx scripts/import-symbols.ts --dry-run --batch-size=30",
    "import:symbols:tpe": "tsx scripts/import-symbols.ts --market=TPE --batch-size=30",
    "import:symbols:us": "tsx scripts/import-symbols.ts --market=US --batch-size=30",
    "import:symbols:jp": "tsx scripts/import-symbols.ts --market=JP --batch-size=30",
    "import:symbols:small": "tsx scripts/import-symbols.ts --batch-size=10",
    "pipeline:full": "npx tsx src/cli-pipeline.ts run",
    "pipeline:data-only": "npx tsx src/cli-pipeline.ts run --skip-crawl",
    "pipeline:crawl-only": "npx tsx src/cli-pipeline.ts run --skip-symbol-import --skip-fundamental-import --skip-label-sync",
    "pipeline:symbols-only": "npx tsx src/cli-pipeline.ts run --skip-crawl --skip-aggregate --skip-fundamental-import --skip-label-sync",
    "pipeline:labels-only": "npx tsx src/cli-pipeline.ts run --skip-crawl --skip-aggregate --skip-symbol-import --skip-fundamental-import",
    "setup:all": "npm run pipeline:data-only",
    "setup:symbols": "npm run generate:mappings && npm run import:symbols && npm run sync:labels",
    "setup:legacy": "npm run generate:mappings && npm run import:symbols && npm run sync:labels:simple",
    "reset:all": "npm run clear:labels:hard && npm run setup:all",
    "reset:soft": "npm run clear:labels && npm run sync:labels",
    "reset:symbols": "npm run clear:labels:hard && npm run setup:symbols"
  },
  "bin": {
    "universal-crawler": "dist/cli.js"
  },
  "keywords": [
    "crawler",
    "scraper",
    "web-scraping",
    "nodejs",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "dotenv": "^16.6.1",
    "fs-extra": "^11.2.0",
    "iconv-lite": "^0.6.3",
    "jsonpath-plus": "^10.3.0",
    "ora": "^7.0.1",
    "playwright": "^1.54.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
