# Database Configuration
POSTGRES_DB_IP=localhost
POSTGRES_DB_PORT=5432
POSTGRES_DB_USER=your_db_user
POSTGRES_DB_PASSWORD=your_db_password
POSTGRES_DB_NAME_AHA=aha-production
POSTGRES_DB_NAME_AHA_DEV=aha-development

# Environment
NODE_ENV=development

# Pipeline Configuration
PIPELINE_BATCH_SIZE=100
PIPELINE_CONCURRENT_CRAWLERS=2
PIPELINE_DELAY_BETWEEN_REQUESTS=3000
PIPELINE_TIMEOUT=30000

# Logging
LOG_LEVEL=info

# Backend API Configuration
BACKEND_API_URL=http://localhost:3000
# API Token for backend authentication (requires ADMIN or SUPER_ADMIN role)
# Used by scripts like import-symbols.ts for bulk operations
# Obtain from: /auth/auto-login endpoint or admin panel
BACKEND_API_TOKEN=
