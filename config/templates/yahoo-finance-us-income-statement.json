    },
    "netNonOperatingInterestIncomeValues": {
      "selector": ".tableBody > div:nth-child(6) > div:nth-child(n+2), tbody > tr:nth-child(6) > td:nth-child(n+2)",
      "multiple": true,
      "transform": "parseUSFinancialValuesArray"
    },
    "otherIncomeExpenseValues": {
