{
  "templateType": "us-income-statement",
  "url": "https://finance.yahoo.com/quote/${symbolCode}/financials/",
  "_note": "US INCOME STATEMENT CONFIG (Unified Array Selectors): Extracts annual income statement data from Yahoo Finance US. Each field uses independent array selectors with multiple:true for unified data extraction. Values are kept in thousands (original format, no conversion). Based on cashflow template architecture.",
  "actions": [
    {
      "type": "wait",
      "timeout": 5000,
      "description": "等待頁面載入"
    }
  ],
  "selectors": {
    "stockInfo": {
      "selector": "h1, section h1, div h1",
      "transform": "cleanStockSymbol"
    },
    "fiscalPeriodsArray": {
      "selector": ".tableHeader > div > div:nth-child(n+2), thead > tr > th:nth-child(n+2)",
      "multiple": true,
      "transform": "parseUSFinancialPeriodsArray"
    },
    "totalRevenueValues": {
      "selector": ".tableBody > div:nth-child(1) > div:nth-child(n+2), tbody > tr:nth-child(1) > td:nth-child(n+2)",
      "multiple": true,
      "transform": "parseUSFinancialValuesArray"
    },
    "costOfRevenueValues": {
      "selector": ".tableBody > div:nth-child(2) > div:nth-child(n+2), tbody > tr:nth-child(2) > td:nth-child(n+2)",
